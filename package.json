{
  "name": "geouax",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start-lint": "npm run lint && ng serve --open",
    "build": "ng build",
    "build-dev": "ng build --configuration development --output-path=dist/geouax/browser",
    "build-prod": "ng build --configuration production --output-path=dist/geouax/browser",
    "watch": "ng build --watch --configuration development --output-path=dist/geouax/browser",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint --fix",
    "firebase-deploy": "firebase deploy --only hosting",
    "deploy": "npm run build && npm run firebase-deploy",
    "deploy-dev": "npm run build-dev && npm run firebase-deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.8",
    "@angular/common": "^19.2.8",
    "@angular/compiler": "^19.2.8",
    "@angular/core": "^19.2.8",
    "@angular/fire": "^19.1.0",
    "@angular/forms": "^19.2.8",
    "@angular/localize": "^19.2.8",
    "@angular/platform-browser": "^19.2.8",
    "@angular/platform-browser-dynamic": "^19.2.8",
    "@angular/router": "^19.2.8",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mapbox/mapbox-gl-geocoder": "^5.0.3",
    "@mapbox/mapbox-sdk": "^0.16.1",
    "@mapbox/polyline": "^1.2.1",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@popperjs/core": "^2.11.8",
    "@primeng/themes": "^19.1.0",
    "@swimlane/ngx-charts": "^22.0.0-alpha.1",
    "@types/leaflet": "^1.9.16",
    "d3": "^7.9.0",
    "firebase": "^11.6.0",
    "leaflet": "^1.9.4",
    "mapbox-gl": "^3.11.0",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.9",
    "@angular/cli": "^19.2.9",
    "@angular/compiler-cli": "^19.2.8",
    "@types/jasmine": "~5.1.0",
    "@types/mapbox__mapbox-gl-geocoder": "^5.0.0",
    "@types/mapbox__polyline": "^1.0.5",
    "angular-eslint": "19.3.0",
    "eslint": "^9.23.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.27.0"
  }
}
